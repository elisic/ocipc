    <sharingModel>ReadWrite</sharingModel>
    <webLinks>
        <fullName>Upload_Picture</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Upload Picture</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var Id = &apos;{!SicProduct__c.Id}&apos;;
location.href = &apos;/apex/elisic__OCI_UploadPictureDocument?id=&apos;+Id;</url>
    </webLinks>
</CustomObject>
